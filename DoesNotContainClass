package samplePrograms;

public class DoesNotContainClass {
	public static void main(String[] args) {
        String sentence = "Java is a programming language.";
        String substring = "not";
        boolean doesNotContainSubstring = !sentence.contains(substring);
        if (doesNotContainSubstring) {
            System.out.println("The sentence does not contain the substring: \"" + substring + "\"");
        } else {
            System.out.println("The sentence contains the substring: \"" + substring + "\"");
        }
    }
}
